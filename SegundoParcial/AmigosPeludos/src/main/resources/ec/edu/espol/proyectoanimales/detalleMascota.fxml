<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="527.0" prefWidth="626.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.espol.proyectoanimales.DetalleMascotaController">
   <children>
      <HBox alignment="CENTER">
         <children>
            <AnchorPane prefHeight="484.0" prefWidth="591.0" style="-fx-background-color: #9ded74;">
               <children>
                  <VBox alignment="CENTER" prefHeight="475.0" prefWidth="586.0">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="66.0" prefWidth="280.0" text="Detalle Mascota" textFill="#252520">
                           <font>
                              <Font name="System Bold" size="18.0" />
                           </font>
                        </Label>
                        <HBox alignment="CENTER">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="33.0" prefWidth="185.0" text="Nombre:" />
                              <Label fx:id="nombre" prefHeight="17.0" prefWidth="147.0" text="Label" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="33.0" prefWidth="185.0" text="Raza:" />
                              <Label fx:id="raza" prefHeight="17.0" prefWidth="147.0" text="Label" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="33.0" prefWidth="185.0" text="Dueño:" />
                              <Label fx:id="dueño" prefHeight="17.0" prefWidth="147.0" text="Label" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="33.0" prefWidth="185.0" text="Fecha Nacimiento:" />
                              <Label fx:id="fecha" prefHeight="17.0" prefWidth="147.0" text="Label" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" />
                        <HBox alignment="CENTER" prefHeight="53.0" prefWidth="390.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#regresar" prefHeight="38.0" prefWidth="117.0" text="Regresar" />
                              <ImageView fx:id="imagenFoto" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
